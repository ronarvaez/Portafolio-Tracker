PORTAFOLIO TRACKER

--SME Portafolio x City--
WHERE concat_ws('-', year, month, day) BETWEEN '2024-01-01' AND '$[YYYY-MM-DD - 2D]' -- Aceleración automática -- Agrupado por Mes
AND DAY(concat_ws('-', year, month, day)) > 25  --Toma los últimos días del mes
